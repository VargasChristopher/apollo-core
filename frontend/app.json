{
  "expo": {
    "name": "Apollo Hub",
    "slug": "apollo-hub",
    "userInterfaceStyle": "automatic",
    "scheme": "apollo",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./assets/images/apollo-logo-dark.png",
    "ios": {
      "icon": "./assets/images/apollo-logo-dark.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/apollo-logo-dark.png",
        "backgroundColor": "#020617"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "name": "Apollo Hub",
      "shortName": "Apollo",
      "display": "standalone",
      "backgroundColor": "#020617",
      "themeColor": "#020617",
      "manifest": {
        "icons": [
          {
            "src": "./assets/images/apollo-icon-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
          },
          {
            "src": "./assets/images/apollo-icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
          },
          {
            "src": "./assets/images/apple-touch-icon.png",
            "sizes": "180x180",
            "type": "image/png",
            "purpose": "any"
          }
        ]
      }
    }
  }
}
